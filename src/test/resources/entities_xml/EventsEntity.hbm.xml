<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.alcereo.entities.EventsEntity" table="events" schema="public" catalog="TestDB">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="name">
            <column name="name" sql-type="text" not-null="true"/>
        </property>
        <set name="processorUsed" inverse="true" table="processors_events" schema="public" catalog="TestDB">
            <key>
                <column name="id_event"/>
            </key>
            <many-to-many not-found="ignore" class="ru.alcereo.entities.ProcessorsEntity">
                <column name="id_processor"/>
            </many-to-many>
        </set>
    </class>
</hibernate-mapping>