<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.alcereo.entities.ParametersEntity" table="parameters" schema="public" catalog="TestDB">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="name">
            <column name="name" sql-type="text" not-null="true"/>
        </property>
        <many-to-one name="command" class="ru.alcereo.entities.CommandsEntity">
            <column name="id_command"/>
        </many-to-one>
    </class>
</hibernate-mapping>