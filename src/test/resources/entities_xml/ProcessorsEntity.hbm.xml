<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.alcereo.entities.ProcessorsEntity" table="processors" schema="public" catalog="TestDB">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="name">
            <column name="name" sql-type="text" not-null="true"/>
        </property>
        <many-to-one name="processorVersion" class="ru.alcereo.entities.ProcessorsVersionsEntity">
            <column name="version_id"/>
        </many-to-one>
        <set name="commands" table="processors_commands" schema="public" catalog="TestDB">
            <key>
                <column name="id_processor"/>
            </key>
            <many-to-many not-found="ignore" class="ru.alcereo.entities.CommandsEntity">
                <column name="id_command"/>
            </many-to-many>
        </set>
        <set name="events" table="processors_events" schema="public" catalog="TestDB">
            <key>
                <column name="id_processor"/>
            </key>
            <many-to-many not-found="ignore" class="ru.alcereo.entities.EventsEntity">
                <column name="id_event"/>
            </many-to-many>
        </set>
    </class>
</hibernate-mapping>