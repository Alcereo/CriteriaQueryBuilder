<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.alcereo.entities.ProcessorsVersionsEntity" table="processors_versions" schema="public"
           catalog="TestDB">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="name">
            <column name="name" sql-type="text" not-null="true"/>
        </property>
        <set name="processorsUsed" inverse="true" lazy="true">
            <key>
                <column name="version_id"/>
            </key>
            <one-to-many not-found="ignore" class="ru.alcereo.entities.ProcessorsEntity"/>
        </set>
    </class>
</hibernate-mapping>